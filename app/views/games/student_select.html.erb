<% provide(:title, "Play") %>
<h1 class =  center jumbotron>Select Student</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:game, url: play_path) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      

      <%= f.submit "Play", class: "btn btn-primary" %>
    <% end %>

    
  </div>
</div>