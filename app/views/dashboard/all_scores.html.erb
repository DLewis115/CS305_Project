
	  <div class="form-group">
  <label for="sel1">Select Chapter:</label>
  <select class="form-control" id="sel1">
    <option>Chapter 4</option>
    
  </select>
</div>
<table class="table table-striped table-bordered table-hover table-condensed">
      <thead>
        <tr>
          <th>StudentID</th>
		  <th>Date Taken</th>
          <th>Score</th>
        </tr>
      </thead>
      <tbody>

	  <% @score.each do |score| %>
        
            <tr>
			<% if Student.find_by(id:score.student_id).active == true %>
		<td><%= scorename(score.student_id) %></td>
		<td><%= score.created_at %></td>
		<td><%= score.entry %></td>
	    </tr>
		<% end %>
        
	  
          <% end %>
    </tbody>
  </table>