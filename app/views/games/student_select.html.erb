<% provide(:title, "Play") %>
<h1 class =  center jumbotron>Select Student</h1>

<div class = "row">
  <div class = "col-md-6 col-md-offset-3">
    <%= form_for(:game, url: play_path) do |f| %>
	  <%= f.label :student %>
	  <%= f.collection_select :student_id, @student,:id,:name, {include_blank: false}, {class: "form-control"} %>
	  
	  <p></p>
	  <%= f.submit "Play", class: "btn btn-success" %>
	 <% end %>
	 
	 </div>
	</div>